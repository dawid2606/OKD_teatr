{
  "name": "npm-watch",
  "version": "0.1.1",
  "description": "run scripts from package.json when files change",
  "main": "index.js",
  "dependencies": {
    "nodemon": "^1.3.8",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "tape": "~2.3.2",
    "markdown-code-blocks": "0.0.1"
  },
  "bin": {
    "npm-watch": "./cli.js"
  },
  "scripts": {
    "test": "tape test/*.js",
    "demo": "markdown-code-blocks -t bash < README.md",
    "exclusions": "bash -c echo 'An extension'"
  },
  "watch": {
    "test": [
      "watch-package.js",
      "test/*.js"
    ],
    "exclusions": {
      "patterns": "*",
      "extensions": "nothing",
      "ignore": "ignored.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/grncdr/npm-watch.git"
  },
